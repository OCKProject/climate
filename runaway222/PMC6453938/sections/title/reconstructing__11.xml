     <span mathvariant="normal" class="mi">Bin</span>
     <div close=")" open="(" separators="" class="mfenced">
      <div class="mrow">
       <div class="msub">
        <div class="mrow">
         <span class="mi">N</span>
        </div>
        <div class="mrow">
         <span mathvariant="normal" class="mi">sim</span>
         <span width="0.3em" class="mspace"/>
         <span class="mn">13</span>
         <span class="mo">,</span>
         <span class="mi">t</span>
         <span class="mo">+</span>
         <span class="mn">1</span>
        </div>
       </div>
