<?xml version="1.0" encoding="UTF-8"?>
<div class="glossary" xmlns="http://www.w3.org/1999/xhtml">
 <div class="title">Abbreviations</div>
 <div class="def-list">
  <div class="def-item">
   <span class="term">MLH</span>
   <div class="def">
    <p>Multi-locus heterozygosity</p>
   </div>
  </div>
  <div class="def-item">
   <span class="term">HFC</span>
   <div class="def">
    <p>Heterozygosity-fitness correlations</p>
   </div>
  </div>
  <div class="def-item">
   <span class="term">IgG</span>
   <div class="def">
    <p>Immunoglobulin G</p>
   </div>
  </div>
  <div class="def-item">
   <span class="term">HWE</span>
   <div class="def">
    <p>Hardy-Weinberg equilibrium</p>
   </div>
  </div>
  <div class="def-item">
   <span class="term">HL</span>
   <div class="def">
    <p>Homozygosity weighted by locus</p>
   </div>
  </div>
  <div class="def-item">
   <span class="term">GLM</span>
   <div class="def">
    <p>Generalised linear model</p>
   </div>
  </div>
  <div class="def-item">
   <span class="term">GLMM</span>
   <div class="def">
    <p>Generalised linear mixed model</p>
   </div>
  </div>
  <div class="def-item">
   <span class="term">SE</span>
   <div class="def">
    <p>Standard error</p>
   </div>
  </div>
  <div class="def-item">
   <span class="term">SD</span>
   <div class="def">
    <p>Standard deviation</p>
   </div>
  </div>
  <div class="def-item">
   <span class="term">LRT</span>
   <div class="def">
    <p>Likelihood ratio test</p>
   </div>
  </div>
 </div>
</div>
