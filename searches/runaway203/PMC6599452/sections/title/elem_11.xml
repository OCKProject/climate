           <div class="msub">
            <div class="mrow">
             <span class="mtext">CS</span>
            </div>
            <div class="mrow">
             <span class="mtext">grain</span>
             <span class="mo">,</span>
             <span class="mi">i</span>
            </div>
           </div>
          </div>
         </div>
        </div>
        <span class="mo">)</span>
       </div>
      </div>
