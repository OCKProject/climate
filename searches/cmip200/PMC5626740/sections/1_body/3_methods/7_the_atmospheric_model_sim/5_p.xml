    <mi class="mi">α</mi>
    <mo class="mo">=</mo>
    <mfenced close=")" open="(" separators="" class="mfenced">
     <mrow class="mrow">
      <mi class="mi">S</mi>
      <mi class="mi">W</mi>
      <mi class="mi">d</mi>
      <mi class="mi">o</mi>
      <mi class="mi">w</mi>
