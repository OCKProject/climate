        <mi mathvariant="normal" class="mi">cSFC</mi>
       </mrow>
      </msub>
     </mrow>
    </mfenced>
    <mo class="mo">∕</mo>
    <mi class="mi">S</mi>
    <mi class="mi">W</mi>
    <mi class="mi">d</mi>
