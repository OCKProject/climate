      <mi mathvariant="normal" class="mi">GPP</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">ENSO</mi>
     </mrow>
    </msubsup>
    <mo class="mo">=</mo>
    <msubsup class="msubsup">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">Δγ</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">GPP</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">Temp</mi>
     </mrow>
    </msubsup>
    <msubsup class="msubsup">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">γ</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">Temp</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">ENSO</mi>
     </mrow>
    </msubsup>
    <mo class="mo">+</mo>
    <msubsup class="msubsup">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">Δγ</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">Temp</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">ENSO</mi>
     </mrow>
    </msubsup>
    <msubsup class="msubsup">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">γ</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">GPP</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">Temp</mi>
     </mrow>
