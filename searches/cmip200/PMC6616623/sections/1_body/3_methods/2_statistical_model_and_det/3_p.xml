      <mtd columnalign="center" class="mtd">
       <msup class="msup">
        <mrow class="mrow">
         <mfenced close=")" open="(" separators="" class="mfenced">
          <mrow class="mrow">
           <mn class="mn">2</mn>
           <mi mathvariant="normal" class="mi">π</mi>
