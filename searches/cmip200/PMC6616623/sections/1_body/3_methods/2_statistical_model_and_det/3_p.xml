          </mrow>
          <mrow class="mrow">
           <mn class="mn">2</mn>
           <msubsup class="msubsup">
            <mrow class="mrow">
             <mover accent="true" class="mover">
              <mrow class="mrow">
               <mi class="mi">σ</mi>
              </mrow>
              <mo class="mo">^</mo>
             </mover>
            </mrow>
            <mrow class="mrow">
