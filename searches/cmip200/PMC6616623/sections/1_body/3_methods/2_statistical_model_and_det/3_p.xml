            </mrow>
            <mo class="mo">^</mo>
           </mover>
          </mrow>
          <mrow class="mrow">
           <mi mathvariant="normal" class="mi">o</mi>
          </mrow>
         </msub>
        </mrow>
       </mfenced>
       <mo class="mo">=</mo>
