<?xml version="1.0" encoding="UTF-8"?>
<glossary class="glossary">
 <title class="title">Abbreviations</title>
 <def-list class="def-list">
  <def-item class="def-item">
   <term class="term">MTE</term>
   <def class="def">
    <p class="p">metabolic theory of ecology</p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">
    <italic class="italic">OA</italic>
   </term>
   <def class="def">
    <p class="p">elevated CO
     <sub class="sub">2</sub>
    </p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">
    <italic class="italic">OAT</italic>
   </term>
   <def class="def">
    <p class="p">elevated CO
     <sub class="sub">2</sub> and temperature
    </p>
   </def>
  </def-item>
  <def-item class="def-item">
   <term class="term">
    <italic class="italic">T</italic>
   </term>
