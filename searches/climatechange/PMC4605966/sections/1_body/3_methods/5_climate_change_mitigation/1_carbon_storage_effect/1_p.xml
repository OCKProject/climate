 <disp-formula id="Equ21" class="disp-formula">
  <label class="label">21</label>
  <alternatives class="alternatives">
   <tex-math id="M41" class="tex-math">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$$ACS_{t,i,l} = CSW_{t,i,l} - CSW_{t - 1,i,l}$$\end{document}</tex-math>
   <math id="M42" display="block" class="math">
