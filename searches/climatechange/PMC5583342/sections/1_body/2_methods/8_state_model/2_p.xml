        <mo class="mo">,</mo>
        <mi class="mi">s</mi>
        <mo class="mo">,</mo>
        <mi class="mi">t</mi>
       </mrow>
      </msub>
      <mi class="mi">exp</mi>
      <mrow class="mrow">
       <mo stretchy="false" class="mo">(</mo>
       <mrow class="mrow">
        <mo class="mo">−</mo>
        <mrow class="mrow">
         <mo stretchy="false" class="mo">(</mo>
         <mrow class="mrow">
          <msub class="msub">
           <mrow class="mrow">
            <mi class="mi">r</mi>
