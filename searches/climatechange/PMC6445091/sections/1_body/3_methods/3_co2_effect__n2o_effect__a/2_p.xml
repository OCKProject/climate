     </mrow>
    </mfrac>
    <mo class="mo">×</mo>
    <mfrac class="mfrac">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">44</mi>
      <mspace width="0.3em" class="mspace"/>
      <mi mathvariant="normal" class="mi">kg</mi>
      <mspace width="0.3em" class="mspace"/>
      <msub class="msub">
       <mrow class="mrow">
        <mi mathvariant="normal" class="mi">CO</mi>
       </mrow>
       <mrow class="mrow">
        <mn class="mn">2</mn>
       </mrow>
      </msub>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">12</mi>
      <mspace width="0.3em" class="mspace"/>
      <mi mathvariant="normal" class="mi">kg</mi>
      <mspace width="0.3em" class="mspace"/>
      <mi mathvariant="normal" class="mi">C</mi>
     </mrow>
    </mfrac>
   </math>
