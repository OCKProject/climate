   <math id="M9" class="math">
    <mrow class="mrow">
     <mrow class="mrow">
      <msub class="msub">
       <mrow class="mrow">
        <mi mathvariant="normal" class="mi">log</mi>
       </mrow>
       <mrow class="mrow">
        <mn class="mn">10</mn>
       </mrow>
      </msub>
     </mrow>
     <mrow class="mrow">
      <mo class="mo">(</mo>
      <mrow class="mrow">
       <mo class="mo">−</mo>
       <msub class="msub">
        <mrow class="mrow">
         <mi class="mi">t</mi>
        </mrow>
        <mrow class="mrow">
         <mn class="mn">0</mn>
        </mrow>
       </msub>
      </mrow>
      <mo class="mo">)</mo>
      <mo class="mo">≈</mo>
      <mo class="mo">−</mo>
      <mn class="mn">0.3922</mn>
      <mo class="mo">−</mo>
      <mn class="mn">0.2752</mn>
      <mo class="mo">×</mo>
      <mrow class="mrow">
       <mrow class="mrow">
        <msub class="msub">
         <mrow class="mrow">
          <mi mathvariant="normal" class="mi">log</mi>
         </mrow>
         <mrow class="mrow">
          <mn class="mn">10</mn>
         </mrow>
        </msub>
       </mrow>
       <mspace width="0.10em" class="mspace"/>
       <mrow class="mrow">
        <msub class="msub">
         <mrow class="mrow">
          <mi class="mi">L</mi>
         </mrow>
