        <mrow class="mrow">
         <mrow class="mrow">
          <msub class="msub">
           <mrow class="mrow">
            <mi mathvariant="normal" class="mi">log</mi>
           </mrow>
           <mrow class="mrow">
            <mn class="mn">10</mn>
           </mrow>
          </msub>
         </mrow>
         <mspace width="0.10em" class="mspace"/>
         <mrow class="mrow">
          <mi class="mi">K</mi>
         </mrow>
        </mrow>
       </mrow>
      </mrow>
     </mrow>
    </mrow>
   </math>
  </alternatives>
