       </mrow>
       <mrow class="mrow">
        <mi class="mi">i</mi>
       </mrow>
      </msub>
     </mrow>
    </msub>
    <mo class="mo">×</mo>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">V</mi>
     </mrow>
     <mrow class="mrow">
      <msub class="msub">
       <mrow class="mrow">
        <mi class="mi">f</mi>
       </mrow>
       <mrow class="mrow">
        <mi class="mi">i</mi>
       </mrow>
      </msub>
     </mrow>
    </msub>
   </math>
   <graphic xlink:href="41598_2018_23101_Article_Equ1.gif" position="anchor" class="graphic" xmlns:xlink="http://www.w3.org/1999/xlink"/>
  </alternatives>
 </disp-formula>where 
 <inline-formula id="IEq1" class="inline-formula">
  <alternatives class="alternatives">
   <tex-math id="M3" class="tex-math">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$${T}_{ef{f}_{i}}$$\end{document}</tex-math>
   <math id="M4" class="math">
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">T</mi>
     </mrow>
     <mrow class="mrow">
      <mi class="mi">e</mi>
      <mi class="mi">f</mi>
      <msub class="msub">
