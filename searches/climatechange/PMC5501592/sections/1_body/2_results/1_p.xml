       <msup class="msup">
        <mi class="mi">e</mi>
        <mrow class="mrow">
         <mn class="mn">2.6419</mn>
         <mo class="mo">−</mo>
         <mn class="mn">0.2534</mn>
         <mo stretchy="false" class="mo">(</mo>
         <mi class="mi">T</mi>
         <mo stretchy="false" class="mo">)</mo>
         <mo class="mo">+</mo>
         <mn class="mn">0.0040</mn>
         <mo stretchy="false" class="mo">(</mo>
