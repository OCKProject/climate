         <mo class="mo">+</mo>
         <msub class="msub">
          <mi class="mi">β</mi>
          <mi class="mi">P</mi>
         </msub>
         <mo stretchy="false" class="mo">(</mo>
         <mi class="mi">P</mi>
         <mo stretchy="false" class="mo">)</mo>
        </mrow>
       </msup>
      </mrow>
     </mfrac>
     <mo class="mo">=</mo>
     <mfrac class="mfrac">
      <mrow class="mrow">
       <msup class="msup">
        <mi class="mi">e</mi>
        <mrow class="mrow">
         <mn class="mn">2.6419</mn>
         <mo class="mo">−</mo>
         <mn class="mn">0.2534</mn>
         <mo stretchy="false" class="mo">(</mo>
         <mi class="mi">T</mi>
         <mo stretchy="false" class="mo">)</mo>
