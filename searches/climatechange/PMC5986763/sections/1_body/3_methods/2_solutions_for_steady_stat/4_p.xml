       </mrow>
       <mrow class="mrow">
        <mi mathvariant="normal" class="mi">l-p</mi>
       </mrow>
      </msub>
      <mo class="mo">+</mo>
      <msub class="msub">
       <mrow class="mrow">
        <mi class="mi">R</mi>
       </mrow>
       <mrow class="mrow">
        <mi mathvariant="normal" class="mi">mic-p</mi>
       </mrow>
      </msub>
     </mrow>
    </mfenced>
    <mo class="mo">∕</mo>
    <mi class="mi">D</mi>
   </math>
