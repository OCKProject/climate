    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mo class="mo">+</mo>
      <mi class="mi">U</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">c-k</mi>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mo class="mo">+</mo>
      <mi class="mi">U</mi>
     </mrow>
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">c-r</mi>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi mathvariant="normal" class="mi">+SOM</mi>
     </mrow>
     <mrow class="mrow">
