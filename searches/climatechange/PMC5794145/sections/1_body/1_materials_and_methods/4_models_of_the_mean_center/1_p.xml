           <mi class="mi">t</mi>
           <mo stretchy="false" class="mo">)</mo>
          </mrow>
          <mrow class="mrow">
           <mi class="mi">S</mi>
           <mo stretchy="false" class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo stretchy="false" class="mo">)</mo>
          </mrow>
