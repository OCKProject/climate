       <mo stretchy="false" class="mo">^</mo>
      </mover>
      <mrow class="mrow">
       <mn class="mn">0</mn>
      </mrow>
      <mn class="mn">2</mn>
     </msubsup>
     <mrow class="mrow">
      <mo stretchy="false" class="mo">(</mo>
      <mi class="mi">T</mi>
      <mo stretchy="false" class="mo">)</mo>
     </mrow>
     <mo class="mo">=</mo>
     <msup class="msup">
      <mi class="mi">a</mi>
      <mn class="mn">2</mn>
     </msup>
     <mrow class="mrow">
      <mo stretchy="false" class="mo">(</mo>
      <mi class="mi">T</mi>
      <mo stretchy="false" class="mo">)</mo>
     </mrow>
     <mfrac class="mfrac">
      <mrow class="mrow">
       <mi class="mi">p</mi>
       <mfenced close=")" open="(" class="mfenced">
        <mi class="mi">T</mi>
       </mfenced>
       <mi class="mi">q</mi>
       <mrow class="mrow">
        <mo stretchy="false" class="mo">(</mo>
        <mi class="mi">T</mi>
        <mo stretchy="false" class="mo">)</mo>
       </mrow>
      </mrow>
      <mrow class="mrow">
       <mi class="mi">δ</mi>
       <mo stretchy="false" class="mo">(</mo>
       <mi class="mi">T</mi>
       <mo stretchy="false" class="mo">)</mo>
       <mi class="mi">ξ</mi>
      </mrow>
     </mfrac>
     <mrow class="mrow"/>
     <mo class="mo">∗</mo>
     <mfrac class="mfrac">
      <mrow class="mrow">
