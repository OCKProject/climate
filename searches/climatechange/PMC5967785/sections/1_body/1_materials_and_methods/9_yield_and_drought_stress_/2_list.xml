 <list-item class="list-item">
  <p class="p">
   <inline-formula id="pone.0196873.e004" class="inline-formula">
    <alternatives class="alternatives">
     <graphic xlink:href="pone.0196873.e004.jpg" id="pone.0196873.e004g" mimetype="image" position="anchor" orientation="portrait" class="graphic" xmlns:xlink="http://www.w3.org/1999/xlink"/>
     <math id="M4" class="math">
