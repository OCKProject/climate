      <mi class="mi">P</mi>
      <mo class="mo">=</mo>
      <mfrac class="mfrac">
       <mrow class="mrow">
        <mi class="mi">G</mi>
        <mi class="mi">Y</mi>
        <mi class="mi">r</mi>
        <mo class="mo">+</mo>
