     </mrow>
     <mrow class="mrow">
      <mi class="mi">a</mi>
     </mrow>
    </msub>
    <mo class="mo">−</mo>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">C</mi>
     </mrow>
     <mrow class="mrow">
      <mi class="mi">i</mi>
     </mrow>
    </msub>
    <mo stretchy="false" class="mo">)</mo>
   </math>
