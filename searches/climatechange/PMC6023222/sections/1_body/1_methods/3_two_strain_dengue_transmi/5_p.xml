          </msub>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo class="mo">)</mo>
          </mrow>
          <msub class="msub">
           <mi class="mi">N</mi>
           <mi class="mi">v</mi>
          </msub>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo class="mo">)</mo>
          </mrow>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <msub class="msub">
            <mi class="mi">k</mi>
            <mi class="mi">l</mi>
           </msub>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <mi class="mi">t</mi>
            <mo class="mo">)</mo>
           </mrow>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <msub class="msub">
