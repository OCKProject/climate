           <mi class="mi">l</mi>
          </msub>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo class="mo">)</mo>
          </mrow>
          <msub class="msub">
           <mi class="mi">ω</mi>
           <mi class="mi">i</mi>
          </msub>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo class="mo">)</mo>
          </mrow>
          <mi class="mi">ν</mi>
         </mrow>
         <mrow class="mrow">
          <msub class="msub">
           <mi class="mi">μ</mi>
