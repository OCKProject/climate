            </msub>
            <mo class="mo">+</mo>
            <msub class="msub">
             <mi class="mi">ω</mi>
             <mi class="mi">i</mi>
            </msub>
            <mrow class="mrow">
             <mo class="mo">(</mo>
             <mi class="mi">t</mi>
             <mo class="mo">)</mo>
            </mrow>
            <mo class="mo">)</mo>
           </mrow>
           <mo class="mo">+</mo>
           <msub class="msub">
            <mi class="mi">μ</mi>
            <mn class="mn">0</mn>
           </msub>
           <msub class="msub">
            <mi class="mi">S</mi>
            <mi class="mi">e</mi>
           </msub>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <mi class="mi">t</mi>
            <mo class="mo">)</mo>
           </mrow>
           <mo class="mo">)</mo>
          </mrow>
         </mrow>
        </mfrac>
        <mo class="mo">,</mo>
       </mrow>
      </mtd>
     </mtr>
     <mtr class="mtr">
      <mtd columnalign="right" class="mtd">
       <msub class="msub">
        <mo class="mo">Λ</mo>
        <mi class="mi">i</mi>
       </msub>
      </mtd>
      <mtd class="mtd">
       <mo class="mo">=</mo>
      </mtd>
      <mtd columnalign="left" class="mtd">
       <mrow class="mrow">
        <mfrac class="mfrac">
         <mrow class="mrow">
          <msub class="msub">
           <mi class="mi">α</mi>
           <mi class="mi">i</mi>
          </msub>
          <msub class="msub">
           <mi class="mi">β</mi>
           <mrow class="mrow">
            <mi class="mi">h</mi>
            <mi class="mi">v</mi>
           </mrow>
          </msub>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo class="mo">)</mo>
          </mrow>
          <msub class="msub">
           <mi class="mi">β</mi>
           <mrow class="mrow">
            <mi class="mi">v</mi>
            <mi class="mi">h</mi>
           </mrow>
          </msub>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo class="mo">)</mo>
          </mrow>
          <mi class="mi">ε</mi>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo class="mo">)</mo>
          </mrow>
          <msub class="msub">
           <mi class="mi">S</mi>
           <mi class="mi">h</mi>
          </msub>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo class="mo">)</mo>
          </mrow>
          <msub class="msub">
           <mi class="mi">S</mi>
           <mi class="mi">v</mi>
          </msub>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo class="mo">)</mo>
          </mrow>
         </mrow>
         <mrow class="mrow">
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <msub class="msub">
            <mi class="mi">α</mi>
            <mi class="mi">i</mi>
           </msub>
           <mo class="mo">+</mo>
           <msub class="msub">
            <mi class="mi">μ</mi>
            <mrow class="mrow">
             <mi class="mi">h</mi>
             <mi class="mi">d</mi>
            </mrow>
           </msub>
           <mo class="mo">)</mo>
          </mrow>
          <msub class="msub">
           <mi class="mi">μ</mi>
           <mi class="mi">v</mi>
          </msub>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">t</mi>
           <mo class="mo">)</mo>
          </mrow>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <mi class="mi">ε</mi>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <mi class="mi">t</mi>
            <mo class="mo">)</mo>
           </mrow>
           <mo class="mo">+</mo>
           <msub class="msub">
            <mi class="mi">μ</mi>
            <mi class="mi">v</mi>
           </msub>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <mi class="mi">t</mi>
            <mo class="mo">)</mo>
           </mrow>
           <mo class="mo">)</mo>
          </mrow>
          <mrow class="mrow">
           <mo class="mo">(</mo>
           <msub class="msub">
            <mi class="mi">μ</mi>
            <mrow class="mrow">
             <mi class="mi">h</mi>
             <mi class="mi">d</mi>
            </mrow>
           </msub>
           <mo class="mo">+</mo>
           <msub class="msub">
            <mi class="mi">γ</mi>
            <mi class="mi">i</mi>
           </msub>
           <mo class="mo">)</mo>
          </mrow>
          <msub class="msub">
           <mi class="mi">N</mi>
           <mi class="mi">h</mi>
          </msub>
          <msup class="msup">
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <mi class="mi">t</mi>
            <mo class="mo">)</mo>
           </mrow>
           <mn class="mn">2</mn>
          </msup>
         </mrow>
        </mfrac>
        <mo class="mo">,</mo>
        <mspace width="4pt" class="mspace"/>
        <mspace width="4pt" class="mspace"/>
        <mtext class="mtext">for</mtext>
        <mspace width="4pt" class="mspace"/>
        <mi class="mi">i</mi>
        <mo class="mo">=</mo>
        <mn class="mn">1</mn>
        <mo class="mo">,</mo>
        <mn class="mn">2</mn>
        <mo class="mo">.</mo>
       </mrow>
      </mtd>
     </mtr>
    </mtable>
   </math>
