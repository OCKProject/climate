      <mi class="mi">k</mi>
     </mrow>
     <mrow class="mrow">
      <mrow class="mrow">
       <mo class="mo">(</mo>
       <mi class="mi">A</mi>
       <mo class="mo">)</mo>
      </mrow>
     </mrow>
    </msubsup>
   </mrow>
  </math>
