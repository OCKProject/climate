       </mrow>
       <mo stretchy="true" class="mo">)</mo>
      </mrow>
     </mrow>
     <mo class="mo">∼</mo>
     <mi class="mi">N</mi>
     <mrow class="mrow">
      <mrow class="mrow">
       <mo class="mo">(</mo>
       <mrow class="mrow">
        <mn class="mn">0</mn>
        <mo class="mo">,</mo>
        <msub class="msub">
         <mi mathvariant="normal" class="mi">Ω</mi>
         <mi class="mi">u</mi>
        </msub>
       </mrow>
       <mo class="mo">)</mo>
      </mrow>
     </mrow>
     <mtext class="mtext"> </mtext>
     <mtext class="mtext"> </mtext>
     <msub class="msub">
      <mrow class="mrow">
       <mtext class="mtext">where</mtext>
       <mtext class="mtext"> </mtext>
       <mtext class="mtext"> </mtext>
       <mi mathvariant="normal" class="mi">Ω</mi>
      </mrow>
      <mi class="mi">u</mi>
     </msub>
     <mo class="mo">=</mo>
     <mrow class="mrow">
      <mrow class="mrow">
       <mo stretchy="true" class="mo">(</mo>
