            <mi class="mi">i</mi>
            <mi class="mi">j</mi>
            <mi class="mi">k</mi>
           </mrow>
           <mrow class="mrow">
            <mrow class="mrow">
             <mo stretchy="true" class="mo">(</mo>
             <mi class="mi">N</mi>
             <mo stretchy="true" class="mo">)</mo>
            </mrow>
