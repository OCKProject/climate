         <mo stretchy="true" class="mo">(</mo>
         <mi class="mi">N</mi>
         <mo stretchy="true" class="mo">)</mo>
        </mrow>
       </mrow>
      </msubsup>
      <mrow class="mrow">
       <mo stretchy="true" class="mo">(</mo>
       <mrow class="mrow">
        <msub class="msub">
         <mi class="mi">P</mi>
         <mrow class="mrow">
          <mi class="mi">i</mi>
          <mi class="mi">j</mi>
         </mrow>
        </msub>
       </mrow>
       <mo stretchy="true" class="mo">)</mo>
      </mrow>
      <mo class="mo">+</mo>
      <msubsup class="msubsup">
       <mi mathvariant="normal" class="mi">β</mi>
       <mrow class="mrow">
        <mn class="mn">4</mn>
        <mi class="mi">k</mi>
       </mrow>
       <mrow class="mrow">
        <mrow class="mrow">
         <mo stretchy="true" class="mo">(</mo>
         <mi class="mi">N</mi>
         <mo stretchy="true" class="mo">)</mo>
        </mrow>
       </mrow>
      </msubsup>
      <mrow class="mrow">
       <mo stretchy="true" class="mo">(</mo>
       <mrow class="mrow">
        <msub class="msub">
         <mi class="mi">G</mi>
         <mrow class="mrow">
          <mi class="mi">i</mi>
          <mi class="mi">j</mi>
         </mrow>
        </msub>
        <mo class="mo">×</mo>
        <msub class="msub">
         <mi class="mi">P</mi>
         <mrow class="mrow">
          <mi class="mi">i</mi>
          <mi class="mi">j</mi>
         </mrow>
        </msub>
       </mrow>
       <mo stretchy="true" class="mo">)</mo>
      </mrow>
      <mo class="mo">+</mo>
      <mi mathvariant="bold" class="mi">B</mi>
      <mrow class="mrow">
       <mo class="mo">·</mo>
       <mi mathvariant="bold" class="mi">R</mi>
      </mrow>
     </mrow>
    </math>
   </alternatives>
  </disp-formula>
