      <mrow class="mrow">
       <mo stretchy="true" class="mo">(</mo>
       <mrow class="mrow">
        <msub class="msub">
         <mi class="mi">G</mi>
         <mrow class="mrow">
          <mi class="mi">i</mi>
          <mi class="mi">j</mi>
