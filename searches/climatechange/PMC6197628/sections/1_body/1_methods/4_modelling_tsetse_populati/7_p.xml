     </mrow>
     <mrow class="mrow">
      <mi class="mi">A</mi>
     </mrow>
    </msub>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">A</mi>
     </mrow>
     <mrow class="mrow">
