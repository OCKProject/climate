           <mi class="mi">P</mi>
          </mrow>
          <mrow class="mrow">
           <msubsup class="msubsup">
            <mrow class="mrow">
             <mi mathvariant="normal" class="mi">PPP</mi>
            </mrow>
            <mrow class="mrow">
