   <tex-math id="M19" class="tex-math">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$${\mathrm{pop}}_{{\mathrm{decade}} = x,{\mathrm{age}}\,{\mathrm{group}}{\mathrm{ = }}y}^{{\mathrm{grid}} = z,{\mathrm{SSP}} = w}$$\end{document}</tex-math>
