      </mtd>
     </mtr>
     <mtr class="mtr">
      <mtd columnalign="left" class="mtd"/>
      <mtd columnalign="left" class="mtd"/>
      <mtd columnalign="left" class="mtd">
       <mo class="mo">+</mo>
       <mfrac class="mfrac">
        <mrow class="mrow">
         <mfenced close="" open="[" separators="" class="mfenced">
          <mrow class="mrow">
           <mfenced close="]" open="[" separators="" class="mfenced">
            <mrow class="mrow">
