                   <mi class="mi">x</mi>
                   <mo class="mo">,</mo>
                   <mi mathvariant="normal" class="mi">IIASA</mi>
                  </mrow>
                  <mrow class="mrow">
                   <mi mathvariant="normal" class="mi">SSP</mi>
                   <mo class="mo">=</mo>
                   <mi class="mi">w</mi>
                  </mrow>
                 </msubsup>
                </mrow>
               </msub>
               <mo class="mo">+</mo>
               <msub class="msub">
                <mrow class="mrow">
                 <mi mathvariant="normal" class="mi">GDP</mi>
                </mrow>
                <mrow class="mrow">
                 <msubsup class="msubsup">
                  <mrow class="mrow">
                   <mi mathvariant="normal" class="mi">PPP</mi>
                  </mrow>
                  <mrow class="mrow">
                   <mi mathvariant="normal" class="mi">decade</mi>
                   <mo class="mo">=</mo>
                   <mi class="mi">x</mi>
                   <mo class="mo">,</mo>
                   <mi mathvariant="normal" class="mi">OECD</mi>
                  </mrow>
                  <mrow class="mrow">
                   <mi mathvariant="normal" class="mi">SSP</mi>
                   <mo class="mo">=</mo>
                   <mi class="mi">w</mi>
                  </mrow>
                 </msubsup>
                </mrow>
               </msub>
              </mrow>
              <mrow class="mrow">
               <mn class="mn">2</mn>
