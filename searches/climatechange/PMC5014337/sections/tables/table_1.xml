     <td align="center" rowspan="1" colspan="1" class="td"/>
     <td align="left" rowspan="1" colspan="1" class="td"/>
    </tr>
    <tr class="tr">
     <td align="left" rowspan="1" colspan="1" class="td">Yes…</td>
     <td align="char" char="." rowspan="1" colspan="1" class="td">6.94</td>
     <td align="char" char="." rowspan="1" colspan="1" class="td">1.93</td>
     <td align="char" char="." rowspan="1" colspan="1" class="td">1.88</td>
     <td align="char" char="." rowspan="1" colspan="1" class="td">1.73</td>
