<boxed-text id="pone.0213300.box001" position="float" orientation="portrait" class="boxed-text">
 <sec id="sec007" class="sec">
  <title class="title">Box 1. Summary description of the fitted Bayesian smoothing model.</title>
  <sec id="sec008" class="sec">
   <title class="title">Bayesian multinomial logistic smoothing model</title>
   <p class="p">
    <disp-formula id="pone.0213300.e018" class="disp-formula">
     <alternatives class="alternatives">
      <graphic xlink:href="pone.0213300.e018.jpg" id="pone.0213300.e018g" mimetype="image" position="anchor" orientation="portrait" class="graphic" xmlns:xlink="http://www.w3.org/1999/xlink"/>
      <math id="M18" class="math">
       <mtable displaystyle="true" class="mtable">
        <mtr class="mtr">
         <mtd columnalign="right" class="mtd">
          <mtext class="mtext">(Likelihood)</mtext>
         </mtd>
         <mtd columnalign="left" class="mtd">
          <mrow class="mrow">
           <mspace width="1.em" class="mspace"/>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <msub class="msub">
             <mi class="mi">n</mi>
             <mrow class="mrow">
              <mn class="mn">1</mn>
              <mo class="mo">,</mo>
              <mi class="mi">t</mi>
             </mrow>
            </msub>
            <mo class="mo">,</mo>
            <mo class="mo">…</mo>
            <mo class="mo">,</mo>
            <msub class="msub">
             <mi class="mi">n</mi>
             <mrow class="mrow">
              <mn class="mn">9</mn>
              <mo class="mo">,</mo>
              <mi class="mi">t</mi>
             </mrow>
            </msub>
            <mo class="mo">)</mo>
           </mrow>
           <mo class="mo">∼</mo>
           <mtext class="mtext">Mult</mtext>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <msub class="msub">
             <mi class="mi">N</mi>
             <mi class="mi">t</mi>
            </msub>
            <mo class="mo">,</mo>
            <msub class="msub">
             <mi class="mi">p</mi>
             <mrow class="mrow">
              <mn class="mn">1</mn>
              <mo class="mo">,</mo>
              <mi class="mi">t</mi>
             </mrow>
            </msub>
            <mo class="mo">,</mo>
            <mo class="mo">…</mo>
            <mo class="mo">,</mo>
            <msub class="msub">
             <mi class="mi">p</mi>
             <mrow class="mrow">
              <mn class="mn">9</mn>
              <mo class="mo">,</mo>
              <mi class="mi">t</mi>
             </mrow>
            </msub>
            <mo class="mo">)</mo>
           </mrow>
           <mo class="mo">,</mo>
          </mrow>
         </mtd>
        </mtr>
        <mtr class="mtr">
         <mtd columnalign="right" class="mtd">
          <mrow class="mrow">
           <mtext class="mtext">(Model</mtext>
           <mspace width="4.pt" class="mspace"/>
           <mtext class="mtext">Specification)</mtext>
          </mrow>
         </mtd>
         <mtd columnalign="left" class="mtd">
          <mrow class="mrow">
           <mspace width="1.em" class="mspace"/>
           <msub class="msub">
            <mi class="mi">p</mi>
            <mrow class="mrow">
             <mi class="mi">d</mi>
             <mo class="mo">,</mo>
             <mi class="mi">t</mi>
            </mrow>
           </msub>
           <mo class="mo">=</mo>
           <mfrac class="mfrac">
            <mrow class="mrow">
             <mo form="prefix" class="mo">exp</mo>
             <mo class="mo">(</mo>
             <msub class="msub">
              <mi class="mi">η</mi>
              <mrow class="mrow">
               <mi class="mi">d</mi>
               <mo class="mo">,</mo>
               <mi class="mi">t</mi>
              </mrow>
             </msub>
             <mo class="mo">)</mo>
            </mrow>
            <mrow class="mrow">
             <mn class="mn">1</mn>
             <mo class="mo">+</mo>
             <msubsup class="msubsup">
              <mo class="mo">∑</mo>
              <mrow class="mrow">
               <mi class="mi">d</mi>
               <mo class="mo">=</mo>
               <mn class="mn">1</mn>
              </mrow>
              <mn class="mn">8</mn>
             </msubsup>
             <mo form="prefix" class="mo">exp</mo>
             <mrow class="mrow">
              <mo class="mo">(</mo>
              <msub class="msub">
               <mi class="mi">η</mi>
               <mrow class="mrow">
                <mi class="mi">d</mi>
                <mo class="mo">,</mo>
                <mi class="mi">t</mi>
               </mrow>
              </msub>
              <mo class="mo">)</mo>
             </mrow>
            </mrow>
           </mfrac>
           <mo class="mo">,</mo>
           <mspace width="1.em" class="mspace"/>
           <msub class="msub">
            <mi class="mi">p</mi>
            <mrow class="mrow">
             <mn class="mn">9</mn>
             <mo class="mo">,</mo>
             <mi class="mi">t</mi>
            </mrow>
           </msub>
           <mo class="mo">=</mo>
           <mfrac class="mfrac">
            <mn class="mn">1</mn>
            <mrow class="mrow">
             <mn class="mn">1</mn>
             <mo class="mo">+</mo>
             <msubsup class="msubsup">
              <mo class="mo">∑</mo>
              <mrow class="mrow">
               <mi class="mi">d</mi>
               <mo class="mo">=</mo>
               <mn class="mn">1</mn>
              </mrow>
              <mn class="mn">8</mn>
             </msubsup>
             <mo form="prefix" class="mo">exp</mo>
             <mrow class="mrow">
              <mo class="mo">(</mo>
              <msub class="msub">
               <mi class="mi">η</mi>
               <mrow class="mrow">
                <mi class="mi">d</mi>
                <mo class="mo">,</mo>
                <mi class="mi">t</mi>
               </mrow>
              </msub>
              <mo class="mo">)</mo>
             </mrow>
            </mrow>
           </mfrac>
           <mo class="mo">,</mo>
          </mrow>
         </mtd>
        </mtr>
        <mtr class="mtr">
         <mtd columnalign="right" class="mtd">
          <mrow class="mrow"/>
         </mtd>
         <mtd columnalign="left" class="mtd">
          <mrow class="mrow">
           <mspace width="1.em" class="mspace"/>
           <msub class="msub">
            <mi class="mi">η</mi>
            <mrow class="mrow">
             <mi class="mi">d</mi>
             <mo class="mo">,</mo>
             <mi class="mi">t</mi>
            </mrow>
           </msub>
           <mo class="mo">=</mo>
           <munderover class="munderover">
            <mo class="mo">∑</mo>
            <mrow class="mrow">
             <mi class="mi">k</mi>
             <mo class="mo">=</mo>
             <mn class="mn">1</mn>
            </mrow>
            <mrow class="mrow">
             <mi class="mi">K</mi>
             <mo class="mo">+</mo>
             <mn class="mn">3</mn>
            </mrow>
           </munderover>
           <msub class="msub">
            <mi class="mi">β</mi>
            <mrow class="mrow">
             <mi class="mi">d</mi>
             <mo class="mo">,</mo>
             <mi class="mi">k</mi>
            </mrow>
           </msub>
           <msub class="msub">
            <mi class="mi">B</mi>
            <mrow class="mrow">
             <mi class="mi">d</mi>
             <mo class="mo">,</mo>
             <mi class="mi">k</mi>
            </mrow>
           </msub>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <mi class="mi">t</mi>
            <mo class="mo">)</mo>
           </mrow>
           <mo class="mo">,</mo>
          </mrow>
         </mtd>
        </mtr>
        <mtr class="mtr">
         <mtd columnalign="right" class="mtd">
          <mrow class="mrow">
           <mtext class="mtext">(Random</mtext>
           <mspace width="4.pt" class="mspace"/>
           <mtext class="mtext">Walk</mtext>
           <mspace width="4.pt" class="mspace"/>
           <mtext class="mtext">Prior)</mtext>
          </mrow>
         </mtd>
         <mtd columnalign="left" class="mtd">
          <mrow class="mrow">
           <mspace width="1.em" class="mspace"/>
           <msub class="msub">
            <mi class="mi">β</mi>
            <mrow class="mrow">
             <mn class="mn">1</mn>
             <mo class="mo">,</mo>
             <mi class="mi">d</mi>
            </mrow>
           </msub>
           <mo class="mo">∼</mo>
           <mi class="mi">U</mi>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <msub class="msub">
             <mi class="mi">c</mi>
             <mn class="mn">0</mn>
            </msub>
            <mo class="mo">,</mo>
            <msub class="msub">
             <mi class="mi">d</mi>
             <mn class="mn">0</mn>
            </msub>
            <mo class="mo">)</mo>
           </mrow>
           <mo class="mo">,</mo>
           <mspace width="1.em" class="mspace"/>
           <msub class="msub">
            <mi class="mi">β</mi>
            <mrow class="mrow">
             <mi class="mi">k</mi>
             <mo class="mo">+</mo>
             <mn class="mn">1</mn>
             <mo class="mo">,</mo>
             <mi class="mi">d</mi>
            </mrow>
           </msub>
           <mo class="mo">=</mo>
           <msub class="msub">
            <mi class="mi">β</mi>
            <mrow class="mrow">
             <mi class="mi">k</mi>
             <mo class="mo">,</mo>
             <mi class="mi">d</mi>
            </mrow>
           </msub>
           <mo class="mo">+</mo>
           <msub class="msub">
            <mi class="mi">ε</mi>
            <mi class="mi">d</mi>
           </msub>
           <mo class="mo">,</mo>
           <mspace width="1.em" class="mspace"/>
           <msub class="msub">
            <mi class="mi">ε</mi>
            <mi class="mi">d</mi>
           </msub>
           <mo class="mo">∼</mo>
           <mi class="mi">N</mi>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <mn class="mn">0</mn>
            <mo class="mo">,</mo>
            <msubsup class="msubsup">
             <mi class="mi">τ</mi>
             <mi class="mi">d</mi>
             <mn class="mn">2</mn>
            </msubsup>
            <mo class="mo">)</mo>
           </mrow>
           <mo class="mo">,</mo>
          </mrow>
         </mtd>
        </mtr>
        <mtr class="mtr">
         <mtd columnalign="right" class="mtd">
          <mtext class="mtext">(Hyper-Prior)</mtext>
         </mtd>
         <mtd columnalign="left" class="mtd">
          <mrow class="mrow">
           <mspace width="1.em" class="mspace"/>
           <msubsup class="msubsup">
            <mi class="mi">τ</mi>
            <mi class="mi">d</mi>
            <mn class="mn">2</mn>
           </msubsup>
           <mo class="mo">∼</mo>
           <mtext class="mtext">IG</mtext>
           <mrow class="mrow">
            <mo class="mo">(</mo>
            <msub class="msub">
             <mi class="mi">a</mi>
             <mn class="mn">0</mn>
            </msub>
            <mo class="mo">,</mo>
            <msub class="msub">
             <mi class="mi">b</mi>
             <mn class="mn">0</mn>
            </msub>
            <mo class="mo">)</mo>
           </mrow>
           <mo class="mo">.</mo>
          </mrow>
         </mtd>
        </mtr>
       </mtable>
      </math>
     </alternatives>
    </disp-formula>
   </p>
  </sec>
