    <msub class="msub">
     <mi class="mi">H</mi>
     <mtext class="mtext">SLR</mtext>
    </msub>
    <mo class="mo">∪</mo>
    <msub class="msub">
     <mi class="mi">H</mi>
     <mtext class="mtext">CUR</mtext>
    </msub>
    <mo class="mo">-</mo>
    <msub class="msub">
     <mi class="mi">H</mi>
     <mtext class="mtext">CUR</mtext>
    </msub>
    <mo stretchy="false" class="mo">)</mo>
    <mo class="mo">∩</mo>
    <msubsup class="msubsup">
     <mi class="mi">H</mi>
     <mtext class="mtext">changed</mtext>
     <mo class="mo">+</mo>
    </msubsup>
    <mo class="mo">=</mo>
    <mi class="mi">e</mi>
   </mrow>
  </math>
 </disp-formula>
 <disp-formula id="ece35129-disp-0010" class="disp-formula">
