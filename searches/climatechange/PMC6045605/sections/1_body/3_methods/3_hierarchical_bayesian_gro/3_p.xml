            <mrow class="mrow">
             <mi class="mi">T</mi>
            </mrow>
            <mo class="mo">¯</mo>
           </mover>
          </mrow>
          <mrow class="mrow">
           <mi class="mi">j</mi>
          </mrow>
         </msub>
        </mrow>
       </mfenced>
       <mo class="mo">+</mo>
       <msub class="msub">
        <mrow class="mrow">
         <mi class="mi">β</mi>
        </mrow>
        <mrow class="mrow">
         <mn class="mn">2</mn>
         <mi class="mi">j</mi>
        </mrow>
       </msub>
       <mfenced close=")" open="(" separators="" class="mfenced">
        <mrow class="mrow">
         <msub class="msub">
          <mrow class="mrow">
           <mi class="mi">P</mi>
          </mrow>
          <mrow class="mrow">
           <mi class="mi">i</mi>
           <mi class="mi">j</mi>
          </mrow>
         </msub>
         <mo class="mo">-</mo>
         <msub class="msub">
          <mrow class="mrow">
           <mover accent="true" class="mover">
            <mrow class="mrow">
             <mi class="mi">P</mi>
            </mrow>
            <mo class="mo">¯</mo>
           </mover>
          </mrow>
          <mrow class="mrow">
           <mi class="mi">j</mi>
          </mrow>
         </msub>
        </mrow>
       </mfenced>
      </mtd>
     </mtr>
    </mtable>
   </math>
   <graphic xlink:href="41467_2018_5132_Article_Equ7.gif" position="anchor" class="graphic" xmlns:xlink="http://www.w3.org/1999/xlink"/>
