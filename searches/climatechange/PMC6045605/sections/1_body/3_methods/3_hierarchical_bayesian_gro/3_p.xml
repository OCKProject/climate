  <alternatives class="alternatives">
   <tex-math id="M23" class="tex-math">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$$\begin{array}{*{20}{c}} {\beta _{0j},\beta _{1j},\beta _{2j},\gamma _{0j},\gamma _{1j},\gamma _{2j}\sim U\left( { - 10^5,10^5} \right)} \end{array}$$\end{document}</tex-math>
