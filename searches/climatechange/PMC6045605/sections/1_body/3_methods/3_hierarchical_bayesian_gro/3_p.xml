       </msub>
       <mfenced close=")" open="(" separators="" class="mfenced">
        <mrow class="mrow">
         <msub class="msub">
          <mrow class="mrow">
           <mi class="mi">T</mi>
          </mrow>
          <mrow class="mrow">
           <mi class="mi">i</mi>
