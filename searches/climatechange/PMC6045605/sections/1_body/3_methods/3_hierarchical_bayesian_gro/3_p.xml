 <disp-formula id="Equ10" class="disp-formula">
  <label class="label">10</label>
  <alternatives class="alternatives">
   <tex-math id="M25" class="tex-math">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$$\begin{array}{*{20}{c}} {\sigma _j^2\sim \mathrm{IG}\left( {10^{ - 5},10^{ - 5}} \right)} \end{array}$$\end{document}</tex-math>
