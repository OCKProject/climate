           <mo stretchy="true" class="mo">)</mo>
          </mrow>
          <mrow class="mrow">
           <mo stretchy="true" class="mo">[</mo>
           <mrow class="mrow">
            <mn class="mn">1</mn>
            <mo class="mo">−</mo>
            <mi class="mi">exp</mi>
            <mrow class="mrow">
             <mo stretchy="true" class="mo">(</mo>
             <mrow class="mrow">
              <mo class="mo">−</mo>
              <mfrac class="mfrac">
