    <td rowspan="2" class="td">
     <inline-formula id="IEq51" class="inline-formula">
      <alternatives class="alternatives">
       <tex-math id="M127" class="tex-math">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$$\frac{E{T}_{a}}{P}=\sqrt{\frac{E{T}_{p}}{P}\,\tanh (\frac{P}{E{T}_{p}})[1-\exp (-\frac{E{T}_{p}}{P})]}$$\end{document}</tex-math>
       <math id="M128" class="math">
