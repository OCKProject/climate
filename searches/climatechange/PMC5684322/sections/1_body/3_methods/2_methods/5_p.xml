        <mi class="mi">j</mi>
        <mi class="mi">k</mi>
       </mrow>
      </msub>
     </mrow>
     <mo stretchy="false" class="mo">)</mo>
    </mrow>
    <mo class="mo">=</mo>
    <msub class="msub">
