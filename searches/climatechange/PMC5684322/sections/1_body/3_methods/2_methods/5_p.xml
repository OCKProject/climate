       </mrow>
      </msub>
     </mrow>
     <mo stretchy="false" class="mo">)</mo>
    </mrow>
    <mo class="mo">+</mo>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">d</mi>
     </mrow>
     <mrow class="mrow">
      <mi class="mi">i</mi>
     </mrow>
    </msub>
    <mo class="mo">+</mo>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">d</mi>
     </mrow>
     <mrow class="mrow">
      <mi class="mi">i</mi>
      <mi class="mi">j</mi>
     </mrow>
    </msub>
    <mo class="mo">+</mo>
    <msub class="msub">
