      <mi class="mi">e</mi>
      <mi class="mi">c</mi>
      <mi class="mi">e</mi>
      <mi class="mi">n</mi>
      <msub class="msub">
       <mrow class="mrow">
        <mi class="mi">t</mi>
       </mrow>
       <mrow class="mrow">
        <mi class="mi">i</mi>
        <mi class="mi">j</mi>
        <mi class="mi">k</mi>
       </mrow>
      </msub>
     </mrow>
     <mo stretchy="false" class="mo">)</mo>
    </mrow>
    <mspace width=".25em" class="mspace"/>
    <mo class="mo">⋅</mo>
    <mspace width=".25em" class="mspace"/>
    <mi class="mi">log</mi>
    <mrow class="mrow">
     <mo stretchy="false" class="mo">(</mo>
     <mrow class="mrow">
      <mi class="mi">a</mi>
      <mi class="mi">g</mi>
      <msub class="msub">
       <mrow class="mrow">
        <mi class="mi">e</mi>
       </mrow>
       <mrow class="mrow">
        <mi class="mi">i</mi>
        <mi class="mi">j</mi>
        <mi class="mi">k</mi>
       </mrow>
      </msub>
     </mrow>
