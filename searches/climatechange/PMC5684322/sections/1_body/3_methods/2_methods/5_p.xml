    <mo class="mo">+</mo>
    <mrow class="mrow">
     <mo stretchy="false" class="mo">(</mo>
     <mrow class="mrow">
      <msub class="msub">
       <mrow class="mrow">
        <mi class="mi">b</mi>
       </mrow>
       <mrow class="mrow">
        <mn class="mn">0</mn>
       </mrow>
      </msub>
      <mo class="mo">+</mo>
