     <mrow class="mrow">
      <mn class="mn">0</mn>
     </mrow>
    </msub>
    <mo class="mo">+</mo>
    <msub class="msub">
     <mrow class="mrow">
      <mi class="mi">a</mi>
     </mrow>
     <mrow class="mrow">
      <mn class="mn">1</mn>
     </mrow>
    </msub>
    <mspace width=".25em" class="mspace"/>
    <mo class="mo">⋅</mo>
    <mspace width=".25em" class="mspace"/>
    <mi mathvariant="italic" class="mi">recen</mi>
    <msub class="msub">
     <mrow class="mrow">
      <mi mathvariant="italic" class="mi">t</mi>
     </mrow>
     <mrow class="mrow">
      <mi class="mi">i</mi>
      <mi class="mi">j</mi>
      <mi class="mi">k</mi>
     </mrow>
    </msub>
    <mo class="mo">+</mo>
    <mrow class="mrow">
     <mo stretchy="false" class="mo">(</mo>
