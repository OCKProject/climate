   <thead>
    <tr>
     <th align="justify" style="background-color:#C0C0C0" rowspan="1" colspan="1">Indicators</th>
     <th align="justify" style="background-color:#C0C0C0" rowspan="1" colspan="1">Definition</th>
     <th align="justify" style="background-color:#C0C0C0" rowspan="1" colspan="1">Composite index</th>
     <th align="justify" style="background-color:#C0C0C0" rowspan="1" colspan="1">Variable
      <a ref-type="table-fn" rid="t001fn001" href="#t001fn001">
       <span class="sup">1</span>
      </a>
     </th>
     <th align="justify" style="background-color:#C0C0C0" rowspan="1" colspan="1">Sources</th>
